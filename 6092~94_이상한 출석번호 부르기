# 6092
n = int(input())
a = input().split()

for i in range(n):
    a[i] = int(a[i])

d = []
for i in range(24):
    d.append(0)

for i in range(n):
    d[a[i]] += 1

for i in range(1, 24):
    print(d[i], end=' ')

# 6093_입력한 번호 반대로 출력
n = int(input())

a = input().split()

for i in range(n-1, -1, -1) :
  print(a[i], end=' ')
  
#6094_가장 낮은 번호 구하기
n = int(input())

a = min(map(int,input().split()))

print(a)
